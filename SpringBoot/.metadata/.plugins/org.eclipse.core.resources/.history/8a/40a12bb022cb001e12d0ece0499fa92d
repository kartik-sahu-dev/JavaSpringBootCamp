package com.in28minutes.rest.webservices.restfulwebservices.user;

import java.time.LocalDate;

public class User {
	
	private Integer id;
	private String name;
	private LocalDate birthDate;
	
	public User(Integer id, String name, LocalDate birthDate) {
		super();
		this.id = id;
		this.name = name;
		this.birthDate = birthDate;
	}
}
